@model IEnumerable<WBKNET.Models.Proker.Update>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Keterangan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tanggal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Program)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Keterangan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tanggal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Program.ProgramID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UpdateID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UpdateID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UpdateID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
